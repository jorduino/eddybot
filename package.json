{
	"name": "eddybot",
	"version": "1.2.4",
	"description": "Discord Bot for fun",
	"main": "bot.js",
	"dependencies": {
		"algebrite": "^1.4.0",
		"chalk": "^5.6.2",
		"discord.js": "^14.25.1",
		"figlet": "^1.9.4",
		"fs": "^0.0.1-security",
		"object-treeify": "^5.0.1"
	},
	"scripts": {
		"test": "bun test",
		"prestart": "bun src/scripts/generate-command-index.ts",
		"start": "bun src/bot.ts",
		"lint": "biome check",
		"format": "bun biome check --write .",
		"prepare": "husky",
		"markdownlint": "markdownlint '**/*.md'",
		"prebuild": "bun src/scripts/generate-command-index.ts",
		"build": "bun run prebuild && bun build --compile --minify --sourcemap ./src/bot.ts --outfile ./dist/mybot"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jorduino/eddybot.git"
	},
	"keywords": [
		"Discord",
		"Bot"
	],
	"author": "Jordan Habash",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/jorduino/eddybot/issues"
	},
	"homepage": "https://github.com/jorduino/eddybot#readme",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@types/bun": "^1.3.6",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"markdownlint-cli": "^0.47.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}